  <%= turbo_frame_tag "questions_section" do %>
  <div>
    Zadaj Pytanie
    <% if user_signed_in? %>
      <%= render "questions/form", venue: @venue %>
    <% else %>
      <div>
        Musisz być zalogowany zeby zadać pytanie
      </div>
    <% end%>
  </div>
  <div class="my-2 pb-2 text-2xl border-b">
    Pytania:
  </div>
  <div class="block overflow-y-auto max-h-140 divide-y">
    <%= render 'questions/questions_list', questions: questions%>
  </div>
<% end%>